{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "maxDuration": 300
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/run/infiniteo",
      "schedule": "0 14 * * *"
    },
    {
      "path": "/api/cron/run/yourops/linkedin",
      "schedule": "0 15 * * 1-5"
    }
  ]
}
