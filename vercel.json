{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/run/infiniteo",
      "schedule": "0 14 * * *"
    },
    {
      "path": "/api/cron/run/yourops",
      "schedule": "0 10 * * 1-5"
    }
  ]
}
